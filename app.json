{
    "expo": {
        "name": "Bookaccio",
        "slug": "bookaccio",
        "version": "0.0.4",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#43646bff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.bugsdev2.bookaccio",
            "versionCode": 4,
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "1c30e353-a54f-45bc-b351-ff43a5dd8bdb"
            }
        }
    }
}
